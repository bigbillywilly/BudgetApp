{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "node --max-old-space-size=2048 ./node_modules/.bin/tsc",
    "start": "node dist/server.js",
    "render-start": "npm run build && npm start",
    "start:dev": "ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migration:run": "ts-node src/database/migrationRunner.ts up",
    "migration:status": "ts-node src/database/migrationRunner.ts status",
    "migration:create": "ts-node src/database/migrationRunner.ts create",
    "migration:validate": "ts-node src/database/migrationRunner.ts validate",
    "migration:rollback": "ts-node src/database/migrationRunner.ts rollback",
    "db:seed": "psql -h $DB_HOST -U $DB_USER -d $DB_NAME -f src/database/seeds/001_sample_data.sql",
    "db:reset": "npm run db:drop && npm run db:create && npm run migration:run && npm run db:seed",
    "db:create": "createdb $DB_NAME",
    "db:drop": "dropdb $DB_NAME --if-exists",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "docker:build": "docker build -t moneywise-api .",
    "docker:run": "docker run -p 5000:5000 --env-file .env moneywise-api",
    "setup": "npm install && npm run migration:run && echo 'Setup complete! Run npm run dev to start the server'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@types/joi": "^17.2.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^5.10.0",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "redis": "^5.6.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-serve-static-core": "^5.0.7",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/papaparse": "^5.3.16",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}